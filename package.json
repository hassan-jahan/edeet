{
  "name": "edeet",
  "version": "0.1",
  "private": false,
  "description": "Modern block based editor for No-Code world",
  "author": "Hassan Jahan <hassan.jahan@gmail.com>",
  "license": "Commercial",
  "repository": {
    "type": "git",
    "url": "https://github.com/hassan-jahan/edeet"
  },
  "bugs": "https://github.com/hassan-jahan/edeet/issues",
  "keywords": [
    "block-based-editor",
    "editor",
    "page-builder",
    "box-option",
    "drag-and-drop",
    "ui",
    "frontend",
    "vue"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare22": "npm run lint:fix",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "dev": "vite --mode dev",
    "build": "npm run clean:dist && vite build --mode prod && vue-tsc --emitDeclarationOnly",
    "build-no-tsc": "npm run clean:dist && vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run clean:coverage && vitest --coverage --mode test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@iconify/iconify": "^1.0",
    "bcx-expression-evaluator": "^1.2.1",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "2.22",
    "dayjs": "^1.11.7",
    "iconify": "^1.4.0",
    "quill": "^1.3.7",
    "slugify": "^1.6.6",
    "styn": "^0.1.2",
    "suneditor": "^2.44.12",
    "v-runtime-template": "^1.10.0",
    "vue-codemirror": "^4.0.6",
    "vue-multiselect": "^2.1.7",
    "vue-quill-editor": "^3.0.6",
    "vuedraggable": "^2.24.3",
    "vuep": "^0.8.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue2": "^2.2",
    "@vitest/coverage-c8": "^0.25.2",
    "eslint": "^8.23.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-vue": "^9.11.0",
    "happy-dom": "^7.7.0",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vitest": "^0.30.1",
    "vue": "^2.7.*",
    "vue-cool-box-selector": "^2.0.1",
    "vue-tsc": "0.40.8"
  },
  "volta": {
    "node": "14.19.1"
  }
}
